---
name: get_accounts
to: https://api.stripe.com/v1/accounts?limit=100
enabled: true
format: https
trigger_condition: true
request_type: Get
callback:
headers: >
  {%- include 'modules/stripe/shared/api_credentials' -%}
  {
    "Content-Type": "application/x-www-form-urlencoded",
    "Authorization": "Bearer {{ context.exports.payments.gateway.secret_key  }}"
  }
---
{% if data.starting_after %}
starting_after={{ data.starting_after }}
{% endif %}
